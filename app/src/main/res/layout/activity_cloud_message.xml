<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.cloudmessagen.view.CloudMessageActivity">

    <TextView
        android:id="@+id/tv_token"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/size_16"
        android:layout_marginTop="96dp"
        android:layout_marginRight="@dimen/size_16"
        android:layout_marginBottom="@dimen/size_16"
        android:text="@string/token"
        android:textSize="@dimen/font_16"
        app:layout_constraintBottom_toTopOf="@id/tv_title_notification"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_title_notification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/size_16"
        android:text="@string/title_notification"
        android:textSize="@dimen/font_16"
        app:layout_constraintBottom_toTopOf="@id/tv_text_notification"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_token" />

    <TextView
        android:id="@+id/tv_text_notification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/size_16"
        android:text="@string/text_notification"
        android:textSize="@dimen/font_16"
        app:layout_constraintBottom_toBottomOf="@id/tv_title_notification"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_title_notification" />

</androidx.constraintlayout.widget.ConstraintLayout>